{{ form_start(form) }}
<table class="table table-bordered">
    <tr>
        <th>Zone</th>
        <th>Document</th>
        <th>Etiquette</th>
    </tr>
    {% for metaLabel in form.metasLabel %}
        <tr>
            <td>
                {{ metaLabel.vars.value.metaDb.zone }}
            </td>
            <td>{{ form_row(metaLabel.documentEnum) }}</td>
            <td>{{ form_row(metaLabel.etiq) }}</td>
        </tr>
    {% endfor %}
</table>
<button class="btn btn-primary" type="submit"><i class="ti ti-device-floppy"></i> Sauvegarder</button>
{{ form_end(form) }}
