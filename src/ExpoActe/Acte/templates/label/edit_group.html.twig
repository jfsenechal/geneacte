{% extends '@ExpoActe/base.html.twig' %}

{% block title %}Etiquette - {{ metaGroupLabel.getiq }}{% endblock %}

{% block body %}

    <div class="card">
        <h3 class="card-header">Etiquettes: {{ metaGroupLabel.getiq }}</h3>
        <div class="card-body">
            {{ form_start(form) }}
            {{ form_errors(form) }}
            {{ form_row(form.getiq) }}
            <button class="btn btn-primary" type="submit"><i class="ti ti-device-floppy"></i> Sauvegarder</button>
            {{ form_end(form) }}
        </div>
    </div>

{% endblock %}
